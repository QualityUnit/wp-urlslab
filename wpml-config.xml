<!-- Allows localization of the content with WPML -->
<wpml-config>
	<gutenberg-blocks>
			<gutenberg-block type="qu/enhanced-faq" translate="1">
					<key name="title"></key>
			</gutenberg-block>
			<gutenberg-block type="qu/enhanced-faq-item" translate="1">
					<key name="question"></key>
					<key name="content"></key>
			</gutenberg-block>
	</gutenberg-blocks>
</wpml-config>
